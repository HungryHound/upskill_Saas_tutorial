<div class="container">
  <div class="row">
    <h3 class="text-center">Matyaka's Data Entry Page</h3>
    
    <div class="col-md-12">
      <div class="well">
        <%= form_for @matyaka_data_record do |f| %>
          <div class="form-group">
            <%= f.label :account_name %>
            <%= f.text_field :account_name, class: 'form-control' %>
          </div>
          
          <div class="form-group">
            <%= f.label :site_url_address %>
            <%= f.text_field :site_url_address, class: 'form-control' %>
          </div>
          
          <div class="form-group">
            <%= f.label :advertising_group %>
            <%= f.text_field :advertising_group, class: 'form-control' %>
          </div>
        
          <div class="form-group">
            <%= f.label :remaining_funds %>
            <%= f.text_field :remaining_funds, class: 'form-control' %>
          </div>
        
        
          <%= f.submit 'Save', class: 'btn btn-default' %>
        <% end %>
      </div>
    </div>
  </div>
</div>